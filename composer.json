{
    "name": "dshoreman/servidor",
    "description": "A tool to aid in server management.",
    "license": "LGPL-2.1-or-later",
    "type": "project",
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/dshoreman/phpmnd"
    }],
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^3.1.0",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^2.0.4",
        "laravel/framework": "^8.43.0",
        "laravel/sanctum": "^2.11.1",
        "laravel/tinker": "^2.6.1",
        "pusher/pusher-php-server": "^7.0.0"
    },
    "require-dev": {
        "dms/phpunit-arraysubset-asserts": "^0.3.0",
        "facade/ignition": "^2.9.0",
        "fakerphp/faker": "^1.14.1",
        "friendsofphp/php-cs-fixer": "^2.19.0",
        "itsgoingd/clockwork": "^5.0.7",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^5.4.0",
        "nunomaduro/larastan": "^0.7.6",
        "nunomaduro/phpinsights": "dev-master",
        "pdepend/pdepend": "2.x-dev",
        "phan/phan": "^4.0.6",
        "phpmd/phpmd": "^2.10.1",
        "phpmetrics/phpmetrics": "^2.7.4",
        "phpstan/phpstan": "^0.12.88",
        "phpstan/phpstan-mockery": "^0.12.13",
        "phpunit/phpunit": "^9.5.4",
        "povils/phpmnd": "^2.4-dev",
        "psalm/plugin-laravel": "^1.4.5",
        "psr/cache": "^2.0.0",
        "spatie/macroable": "^2.0.0",
        "squizlabs/php_codesniffer": "^3.6.0",
        "vimeo/psalm": "^4.7.3"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "PHPMD\\Rule\\Servidor\\": "build/phpmd/",
            "Servidor\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
