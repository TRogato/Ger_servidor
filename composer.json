{
    "name": "dshoreman/servidor",
    "description": "A tool to aid in server management.",
    "license": "LGPL-2.1-or-later",
    "type": "project",
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^3.5.1",
        "fruitcake/laravel-cors": "^2.0.4",
        "laravel/framework": "^8.70.1",
        "laravel/sanctum": "^2.12.1",
        "laravel/tinker": "^2.6.2",
        "pusher/pusher-php-server": "^7.0.1",
        "spatie/data-transfer-object": "^3.7.2"
    },
    "require-dev": {
        "dms/phpunit-arraysubset-asserts": "^0.3.1",
        "facade/ignition": "^2.16.0",
        "fakerphp/faker": "^1.16.0",
        "friendsofphp/php-cs-fixer": "^3.2.1",
        "itsgoingd/clockwork": "^5.1.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10.0",
        "nunomaduro/larastan": "0.7.15",
        "nunomaduro/phpinsights": "2.0.1",
        "phan/phan": "^5.2.1",
        "phpmd/phpmd": "^2.10.2",
        "phpmetrics/phpmetrics": "^2.7.4",
        "phpstan/phpstan": "^0.12.99",
        "phpstan/phpstan-mockery": "^0.12.14",
        "phpunit/phpunit": "^9.5.10",
        "povils/phpmnd": "^3.0.1",
        "psalm/plugin-laravel": "^1.5.1",
        "spatie/macroable": "^2.0.0",
        "squizlabs/php_codesniffer": "^3.6.1",
        "vimeo/psalm": "^4.12.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "PHPMD\\Rule\\Servidor\\": "build/phpmd/",
            "Servidor\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
